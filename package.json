    "@types/supertest": "^6.0.3",
    "@types/user-agents": "^1.0.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
