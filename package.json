    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cron": "^2.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/mime-types": "^2.1.4",
    "@types/moment": "^2.11.29",
