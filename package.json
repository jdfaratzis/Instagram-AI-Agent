{
  "name": "radiant14-instagram-ai",
  "version": "1.0.0",
  "description": "Instagram AI Agent for Radiant14",
  "main": "build/index.js",
  "scripts": {
    "start": "tsc && node build/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "monitor": "node build/scripts/monitor.js",
    "deploy": "npm run build && pm2 start build/server.js --name instagram-ai-agent",
    "stop": "pm2 stop instagram-ai-agent",
    "restart": "pm2 restart instagram-ai-agent",
    "logs": "pm2 logs instagram-ai-agent",
    "setup": "npm install && npm run build && pm2 startup && pm2 save"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
